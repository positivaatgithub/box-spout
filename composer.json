{
    "name": "positivaatgithub/box-spout",
    "description": "PHP 8.3+ Library to read and write spreadsheet files (CSV, XLSX and ODS), in a fast and scalable way",
    "type": "library",
    "keywords": [
        "php",
        "spreadsheet",
        "csv",
        "xlsx",
        "ods",
        "excel",
        "reader",
        "writer",
        "streaming"
    ],
    "license": "Apache-2.0",
    "homepage": "https://github.com/positivaatgithub/box-spout",
    "authors": [
        {
            "name": "Positiva",
            "email": "zan.jankovec@positiva.com"
        }
    ],
    "require": {
        "php": ">=8.3",
        "ext-zip": "*",
        "ext-xmlreader": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Box\\Spout\\": "src/Spout/"
        }
    }
}

